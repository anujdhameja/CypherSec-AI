{
  "replica_system": {
    "name": "Replica Testing Pipeline",
    "version": "1.0",
    "description": "End-to-end testing system for multi-language vulnerability detection",
    "base_model": "production_model_config9_v1.0.pth",
    "expected_accuracy": 0.8357
  },
  "devign": {
    "learning_rate": 0.0001,
    "weight_decay": 1e-06,
    "loss_lambda": 1e-06,
    "model": {
      "gated_graph_conv_args": {
        "out_channels": 384,
        "num_layers": 4,
        "aggr": "add",
        "bias": true
      },
      "conv_args": {
        "conv1d_1": {
          "in_channels": 100,
          "out_channels": 50,
          "kernel_size": 3,
          "padding": 1
        },
        "conv1d_2": {
          "in_channels": 50,
          "out_channels": 20,
          "kernel_size": 1,
          "padding": 1
        },
        "maxpool1d_1": {
          "kernel_size": 3,
          "stride": 2
        },
        "maxpool1d_2": {
          "kernel_size": 2,
          "stride": 2
        }
      },
      "emb_size": 101
    }
  },
  "create": {
    "filter_column_value": {
      "project": "replica_test"
    },
    "slice_size": 50,
    "joern_cli_dir": "joern/joern-cli/"
  },
  "paths": {
    "cpg": "data/replica_cpg/",
    "joern": "data/replica_joern/",
    "raw": "data/replica_raw/",
    "input": "data/replica_input/",
    "model": "models/",
    "tokens": "data/replica_tokens/",
    "w2v": "data/replica_w2v/"
  },
  "files": {
    "raw": "replica_dataset.json",
    "cpg": "replica_cpg",
    "tokens": "replica_tokens.pkl",
    "w2v": "replica_w2v.model",
    "input": "replica_input.pkl",
    "model": "replica_model.pth"
  },
  "embed": {
    "nodes_dim": 205,
    "word2vec_args": {
      "vector_size": 100,
      "alpha": 0.01,
      "window": 5,
      "min_count": 3,
      "sample": 1e-05,
      "workers": 4,
      "sg": 1,
      "hs": 0,
      "negative": 5
    },
    "edge_type": "Ast"
  },
  "process": {
    "epochs": 5,
    "patience": 3,
    "batch_size": 16,
    "dataset_ratio": 1.0,
    "shuffle": true,
    "gradient_clip": 1.0
  },
  "test_languages": ["c", "cpp", "csharp", "python", "java", "php"],
  "validation_thresholds": {
    "min_accuracy": 0.75,
    "min_cpg_success_rate": 0.90,
    "min_embedding_quality": 0.80
  }
}