{
  "devign": {
    "learning_rate": 0.0001,
    "weight_decay": 1e-06,
    "loss_lambda": 1e-06,
    "model": {
      "gated_graph_conv_args": {
        "out_channels": 256,
        "num_layers": 5,
        "aggr": "add",
        "bias": true
      },
      "conv_args": {
        "conv1d_1": {
          "in_channels": 100,
          "out_channels": 50,
          "kernel_size": 3,
          "padding": 1
        },
        "conv1d_2": {
          "in_channels": 50,
          "out_channels": 20,
          "kernel_size": 1,
          "padding": 1
        },
        "maxpool1d_1": {
          "kernel_size": 3,
          "stride": 2
        },
        "maxpool1d_2": {
          "kernel_size": 2,
          "stride": 2
        }
      },
      "emb_size": 101
    }
  },
  "create": {
    "filter_column_value": {
      "project": "FFmpeg"
    },
    "slice_size": 100,
    "joern_cli_dir": "joern/joern-cli/"
  },
  "paths": {
    "cpg": "data/cpg/",
    "joern": "data/joern/",
    "raw": "data/raw/",
    "input": "data/input/",
    "model": "models/",
    "tokens": "data/tokens/",
    "w2v": "data/w2v/"
  },
  "files": {
    "raw": "test_multilang_sorted.json",
    "cpg": "cpg",
    "tokens": "tokens.pkl",
    "w2v": "w2v.model",
    "input": "input.pkl",
    "model": "final_model.pth"
  },
  "embed": {
    "nodes_dim": 205,
    "word2vec_args": {
      "vector_size": 100,
      "alpha": 0.01,
      "window": 5,
      "min_count": 3,
      "sample": 1e-05,
      "workers": 4,
      "sg": 1,
      "hs": 0,
      "negative": 5
    },
    "edge_type": "Ast"
  },
  "process": {
    "epochs": 100,
    "patience": 15,
    "batch_size": 32,
    "dataset_ratio": 1.0,
    "shuffle": true,
    "gradient_clip": 1.0
  }
}